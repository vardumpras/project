{% extends "::base.html.twig" %}

{% block titleBase %}{% block title %}Boutique Test{% endblock %}{% endblock %}

{% block head %}
    <meta name="description" content="Boutique Test"/>
    <meta name="robots" content="index,follow"/>
    <meta name="viewport" content="width=device-width, minimum-scale=0.25, maximum-scale=1.6, initial-scale=1.0"/>
    <meta name="apple-mobile-web-app-capable" content="yes"/>
    <link rel="icon" type="image/vnd.microsoft.icon" href="/boutique/template1/favicon.ico"/>
    <link rel="shortcut icon" type="image/x-icon" href="/boutique/template1/favicon.ico"/>
    <link rel="stylesheet" href="/boutique/template1/main.css" type="text/css" media="all"/>
    <link rel="stylesheet" href="//fonts.googleapis.com/css?family=Open+Sans:300,600&subset=latin,latin-ext"
          type="text/css" media="all"/>
{% endblock %}

{% block bodyBase %}
    <style>
        a {
            color: #fff;
        }

        a:focus, a:hover {
            text-decoration: none;
        }

        .text-white {
            color: #fff;
        }

        .header-container .container {
            padding: 0px;
        }

        .header-container .container .nav {
            padding: 0 15px;
            background: #333 none repeat scroll 0 0;
        }

        .header-phone {
            float: left;
            line-height: 18px;
            padding: 5px 10px;
            color: #777;
        }

        .header-phone span {
            color: #fff;
            font-size: 21px;
            line-height: 21px;
            padding-right: 7px;
        }

        .header-phone strong {
            color: #fff;
        }

        .header-link {
            border-left: 1px solid #515151;
            float: right;
        }

        .header-link a {
            color: #fff;
            cursor: pointer;
            display: block;
            font-weight: bold;
            line-height: 18px;
            padding: 8px 10px 11px;
            text-shadow: 1px 1px rgba(0, 0, 0, 0.2);
        }

        .header-link a:hover, .header-link a:active {
            background: #2b2b2b none repeat scroll 0 0 !important;
        }

        .header-logo {
            padding: 15px;
        }

        #searchbox {
            padding-top: 50px;
        }

        #searchbox input {
            background: #fbfbfb none repeat scroll 0 0;
            border-radius: 0;
            height: 45px;
        }

        #searchbox .glyphicon {
            border-radius: 0;
            color: #fff;
            top: 0px;
            background: #333 none repeat scroll 0 0;
        }

        .shopping_cart {
            float: right;
            padding-left: 16px;
            padding-right: 16px;
            padding-top: 50px;
            width: 100%;
        }

        .shopping_cart > a {
            background: #333 none repeat scroll 0 0;
            color: #777777;
            display: block;
            font-weight: bold;
            overflow: hidden;
            padding: 10px 16px;
            text-shadow: 1px 1px rgba(0, 0, 0, 0.2);
        }
        .shopping_cart > a b {
            color: #fff;
            font: 600 18px/22px "Open Sans", sans-serif;
            padding-right: 5px;
        }

        .shopping_cart > a .glyphicon-shopping-cart {
            margin-right: 10px;
            color: #FFF;
            font-size: 1.5em;
        }

        .shopping_cart > a .glyphicon-triangle-bottom, .shopping_cart > a .glyphicon-triangle-top {
            float: right;
            padding-top: 7px;
        }

        .product-name {
            color: #3a3939;
            font-size: 17px;
            line-height: 23px;
            margin-bottom: 0;
        }

        .shopping_cart .cart_block {
            opacity: 0;
            height: auto;
            width: 100%;
            -webkit-transition: all 500ms ease-in-out;
            -moz-transition: all 500ms ease-in-out;
            -ms-transition: all 500ms ease-in-out;
            -o-transition: all 500ms ease-in-out;
            transition: all 500ms ease-in-out;
            background: #484848 none repeat scroll 0 0;
            color: #fff;
            position: absolute;
            top: 95px;

            z-index: 100;
            overflow: hidden;
        }

        .shopping_cart > a:hover + .cart_block {
            opacity: 1;
        }

        .shopping_cart .cart_block a {
            color: #fff;
        }

        .shopping_cart .cart_block .quantity-formated {
            color: #9c9b9b;
            font-size: 10px;
            padding-right: 5px;
        }

        .shopping_cart .cart_block .product-name {
            margin-top: -4px;
            padding-bottom: 5px;
        }

        .shopping_cart .cart_block .product-atributes {
            font-size: 11px;
            padding-bottom: 10px;
            color: #9c9b9b;
        }

        .cart_block dt {
            background: #484848 url("/boutique/template1/images/cart-shadow.png") repeat-x scroll 0 0;
            font-weight: normal;
            overflow: hidden;
            padding: 20px 10px 16px 20px;
            position: relative;
        }

        .cart_block dt.firt {
            background: rgba(0, 0, 0, 0) none repeat scroll 0 0;
        }

        .cart_block .cart-images {
            float: left;
            margin-right: 20px;
        }

        .cart_block .cart-info {
            overflow: hidden;
            padding-right: 20px;
            position: relative;
        }

        .cart_block .remove_link {
            position: absolute;
            right: 10px;
            top: 19px;
        }

        .cart_block .cart-prices {
            background: #3d3d3d url("/boutique/template1/images/cart-shadow.png") repeat-x scroll 0 0;
            border: medium none;
            font-weight: bold;
            padding: 10px 20px 22px;
        }

        .cart_block .price {
            float: right;
        }

        .cart_block .cart-buttons {
            background: #333 url("/boutique/template1/images/cart-shadow.png") repeat-x scroll 0 0;
            margin: 0;
            overflow: hidden;
            padding: 20px 20px 10px;
        }

        .cart_block .cart-buttons a {
            text-align: center;
            padding: 0;
            width: 100%;
        }

        .cart_block .cart-buttons a span {
            background: #43b155 none repeat scroll 0 0;
            border: 1px solid #63c473;
            font-size: 1.1em;
            display: block;
            font-weight: bold;
            padding: 7px 0;
        }

        .cart_block .cart-buttons a:hover span {
            background: #2e7a3a none repeat scroll 0 0;
            border: 1px solid #358c43;
            color: #fff;
        }

        .block_menu .cat-title {
            background: #f6f6f6 none repeat scroll 0 0;
            border-bottom: 3px solid #e9e9e9;
            color: #484848;
            display: block;
            font: 600 18px/22px "Open Sans", sans-serif;
            padding: 17px 20px;
            position: relative;
            text-transform: uppercase;
        }
    </style>
